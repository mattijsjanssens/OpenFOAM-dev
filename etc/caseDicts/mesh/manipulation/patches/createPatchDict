{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pointSync   false;

patches
(
    {
        name    inlet;
        patchInfo
        {
            type    patch;
        }
        constructFrom set;
        set     inletFaces;
    }

    {
        name    walls;
        patchInfo
        {
            type    wall;
        }
        constructFrom patches;
        patches ("CAD.*");
    }
);

